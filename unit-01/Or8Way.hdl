/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a = in[0], b = in[1], out = a1);
    Or(a = a1, b = in[2], out = a2);
    Or(a = a2, b = in[3], out = a3);
    Or(a = a3, b = in[4], out = a4);
    Or(a = a4, b = in[5], out = a5);
    Or(a = a5, b = in[6], out = a6);
    Or(a = a6, b = in[7], out = out);
}